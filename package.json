{
  "name": "ak-fetch",
  "version": "1.0.893",
  "description": "a batching queuing fetch the ak way",
  "main": "index.js",
  "scripts": {
    "test": "jest --testPathPattern='test/(unit|integration)' --verbose",
    "test:unit": "jest --testPathPattern='test/unit' --verbose",
    "test:integration": "jest --testPathPattern='test/integration' --verbose",
    "test:watch": "jest --watch --testPathPattern='test/(unit|integration)'",
    "test:coverage": "jest --coverage --testPathPattern='test/(unit|integration)' && open ./coverage/lcov-report/index.html",
    "test:ci": "jest --ci --coverage --testPathPattern='test/(unit|integration)'",
    "post": "npm publish",
    "prune": "rm -rf logs/*",
    "coverage": "npm run test:coverage",
    "scratch": "nodemon scratch.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ak--47/ak-fetch.git"
  },
  "bin": {
    "ak-fetch": "./index.js"
  },
  "author": "AK",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ak--47/ak-fetch/issues"
  },
  "homepage": "https://github.com/ak--47/ak-fetch#readme",
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "nock": "^13.3.8",
    "supertest": "^6.3.4"
  },
  "dependencies": {
    "ak-tools": "^1.0.68",
    "dotenv": "^16.4.5",
    "fetch-retry": "^6.0.0",
    "form-data": "^4.0.0",
    "querystring": "^0.2.1",
    "readline": "^1.3.0",
    "run-queue": "^2.0.1",
    "tough-cookie": "^4.1.3",
    "undici": "^6.6.0",
    "yargs": "^17.7.2"
  }
}
